<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="biz.sendyou.senduandroid.Activity.LoginActivity">


    <ImageView
        android:layout_width="17.5dp"
        android:layout_height="17.5dp"
        android:id="@+id/imageView12"
        android:src="@drawable/la_idicon"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView14"
        android:layout_alignLeft="@+id/imageView14"
        android:layout_marginTop="220dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_email_edittext"
        android:hint="이메일"
        android:background="@android:color/transparent"
        android:layout_alignParentLeft="false"
        android:layout_toRightOf="@+id/imageView12"
        android:layout_alignTop="@+id/imageView12"
        android:layout_marginLeft="15dp"
        android:layout_alignEnd="@+id/signup_password_edittext"
        android:inputType="textEmailAddress" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView13"
        android:src="@drawable/la_line1"
        android:layout_below="@+id/signup_email_edittext"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="4dp" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:id="@+id/imageView14"
        android:src="@drawable/la_pwicon"
        android:layout_below="@+id/imageView13"
        android:layout_alignStart="@+id/imageView13"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="2dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_password_edittext"
        android:hint="비밀번호"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageView14"
        android:layout_toRightOf="@+id/imageView14"
        android:layout_marginLeft="15dp"
        android:layout_alignEnd="@+id/imageView15"
        android:inputType="textPassword" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView15"
        android:src="@drawable/la_line1"
        android:layout_below="@+id/signup_password_edittext"
        android:layout_alignLeft="@+id/imageView13"
        android:layout_marginTop="4dp" />

    <ImageView
        android:layout_width="287.5dp"
        android:layout_height="42.5dp"
        android:id="@+id/signup_next_imageview"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="52dp"
        android:src="@drawable/la_next" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:id="@+id/imageView17"
        android:src="@drawable/la_pwicon"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="2dp"
        android:layout_below="@+id/imageView15"
        android:layout_alignStart="@+id/imageView15" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_password_check_edittext"
        android:hint="비밀번호확인"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageView17"
        android:layout_toRightOf="@+id/imageView17"
        android:layout_marginLeft="15dp"
        android:layout_alignEnd="@+id/imageView18"
        android:inputType="textPassword" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView18"
        android:src="@drawable/la_line1"
        android:layout_below="@+id/signup_password_check_edittext"
        android:layout_alignLeft="@+id/imageView15"
        android:layout_marginTop="4dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/imageView19"
        android:src="@drawable/la_plane"
        android:layout_marginTop="26dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView13" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_first_name_edittext"
        android:layout_alignTop="@+id/imageView19"
        android:layout_alignStart="@+id/imageView19"
        android:layout_marginLeft="170dp"
        android:background="@android:color/transparent"
        android:drawableBottom="@drawable/la_line1"
        android:hint="성"
        android:layout_marginTop="40dp" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_name_edittext"
        android:layout_marginLeft="170dp"
        android:background="@android:color/transparent"
        android:drawableBottom="@drawable/la_line1"
        android:hint="이름"
        android:layout_below="@+id/signup_first_name_edittext"
        android:layout_alignStart="@+id/imageView19"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText6"
        android:layout_marginLeft="170dp"
        android:background="@android:color/transparent"
        android:layout_alignStart="@+id/imageView19"
        android:layout_alignTop="@+id/signup_month_edittext"
        android:layout_marginTop="3dp"
        android:text="생일 :"
        android:textColor="#000000" />

    <EditText
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_month_edittext"
        android:layout_marginLeft="170dp"
        android:background="@android:color/transparent"
        android:drawableBottom="@drawable/la_line1"
        android:layout_below="@+id/signup_name_edittext"
        android:layout_alignStart="@+id/signup_password_edittext"
        android:layout_marginTop="10dp"
        android:inputType="number"
        android:maxLength="2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/editText6"
        android:layout_toEndOf="@+id/signup_month_edittext"
        android:textColor="#000000"
        android:text="월"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp" />

    <EditText
        android:layout_width="20dp"
        android:layout_height="wrap_content"
        android:id="@+id/signup_date_edittext"
        android:background="@android:color/transparent"
        android:drawableBottom="@drawable/la_line1"
        android:layout_alignTop="@+id/signup_month_edittext"
        android:layout_toEndOf="@+id/textView7"
        android:inputType="number"
        android:maxLength="2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:background="@android:color/transparent"
        android:textColor="#000000"
        android:text="일"
        android:layout_alignTop="@+id/textView7"
        android:layout_toEndOf="@+id/signup_date_edittext"
        android:layout_marginLeft="3dp" />

</RelativeLayout>
