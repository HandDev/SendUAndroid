<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#ffffff"/>

            <padding
                android:bottom="50dp"
                android:left="50dp"
                android:right="50dp"
                android:top="50dp"/>

            <stroke
                android:width="20dp"
                android:color="#8cad6b"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="#8cad6b"/>

            <size
                android:width="100dp"
                android:height="100dp"/>
        </shape>

    </item>
</layer-list>